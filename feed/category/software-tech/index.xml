---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>{{ site.title }}</title>
  <link href="{{ "/feed/category/software-tech/" | relative_url }}" rel="self"/>
  <link href="{{ "" | relative_url }}/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ "/feed/category/software-tech" | relative_url }}/</id>
  <author>
    <name>{{ site.author.name }}</name>
  </author>

  {% for post in site.categories["Software/Tech"] %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="{{ post.url | relative_url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ post.url | relative_url }}</id>
    {% if post.category %}<category term="{{ post.category | slugify }}" label="{{ post.category }}"></category>{% endif %}
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
