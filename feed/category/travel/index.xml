---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{ site.title }}</title>
 <link href="{{ "/feed/category/travel/" | absolute_url }}" rel="self"/>
 <link href="{{ site.url | absolute_url }}/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}/</id>
 <author>
   <name>{{ site.author.name }}</name>
 </author>

 {% for post in site.categories["Travel"] %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="{{ post.url | relative_url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ site.url }}{{ post.id }}</id>
   {% if post.category %}<category term="{{ post.category | slugify }}" label="{{ post.category }}"></category>{% endif %}
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
